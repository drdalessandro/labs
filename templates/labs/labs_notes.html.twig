{% if field.hide %}
<tr class="hide">
{% else %}
<tr>
{% endif %}
    <td class="graph" id="{{ field.input|attr }}"><span class="labs-title">{{ field.title|text }}</span></td>
    <td></td>
    <td></td>

    {% if is_edit %}
    <td class='currentvalues p-2'>
        <textarea class="form-control" name='{{ field.input|attr}}' id='{{ field.input|attr }}_input'>{% if attribute(labs, field.vitalsValue) != 0 %}{{ attribute(labs, field.vitalsValue)|text }}{% endif %}</textarea>
    </td>
    <td class="editonly actions">
    </td>
    {% endif %}
    {% for result in results %}
        <td  class='historicalvalues {% if is_edit %}d-none d-md-table-cell{% endif %}'>
            {% if attribute(result, field.vitalsValue) != 0 %}
                {{ attribute(result, field.vitalsValue)|text }}
            {% endif %}
        </td>
    {% endfor %}
</tr>
